<div style='background-color:white;color:black;'>

  <h1>Crea Usuarios</h1>

  <form action="Usuario/CrearUsuarioForm" method="post">
    <div class="form-group">
      <label for="exampleInputEmail1">Nombres</label>
      <input minlength=5 required type="text" class="form-control"  placeholder="Ingrese sus nombres" name="nombres">
    </div>
    <div class="form-group">
      <label for="exampleInputEmail1">Apellidos</label>
      <input minlength=5 required type="text" class="form-control"  placeholder="Ingrese susus apellidos" name="apellidos">
    </div>
    <div class="form-group">
      <label for="exampleInputEmail1">Correo</label>
      <input type="email" class="form-control"  placeholder="Ingrese su correo" name="correo">
    </div>
    <button type="submit" class="btn btn-success">Crear Usuario</button>
    <br>

  </form>
</div>